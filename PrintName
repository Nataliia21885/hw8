public class PrintName {
    public static void main(String[] args) {
        Shape circle = new Circle(5, 10, 8);
        Shape rectangle = new Rectangle(10, 15, 20, 12);
        Shape quad = new Quad(5, 8,15);
        Shape rightTriangle = new RightTriangle(5, 8, 10, 8, 6);
        Shape hexagon = new Hexagon(9, 8, 10);
        System.out.println(circle.getName());
        System.out.println(rectangle.getName());
        System.out.println(quad.getName());
        System.out.println(rightTriangle.getName());
        System.out.println(hexagon.getName());
    }
}
